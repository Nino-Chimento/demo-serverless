{
  "name": "serverless-demo",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest --verbose --runInBand",
    "test-coverage": "jest --coverage=true",
    "test-update-snap": "jest --updateSnapshot",
    "lint": "eslint ./app",
    "lint-fix": "eslint ./app --fix",
    "offline": "npx serverless offline start --noPrependStageInUrl",
    "deploy": "npx serverless deploy --stage ${1}",
    "delete": "npx serverless remove --stage ${1}",
    "create-cert": "npx serverless create-cert --stage ${1}",
    "delete-cert": "npx serverless remove-cert --stage ${1}",
    "deploy-function": "npx serverless deploy function -f trigger-file --stage ${1}"
  },
  "keywords": [],
  "author": "sparkfabrik",
  "license": "ISC",
